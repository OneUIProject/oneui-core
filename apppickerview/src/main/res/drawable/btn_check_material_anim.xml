<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@id/on"
        android:state_checked="true">

        <bitmap
            android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_015"
            android:tint="@color/sesl_check_on" />

    </item>

    <item
        android:id="@+id/off">

        <bitmap
            android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_000"
            android:tint="@color/sesl_check_off" />

    </item>

    <transition
        android:toId="@id/on" android:fromId="@id/off">

        <animation-list>
            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_000"
                    android:tint="@color/sesl_check_off" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_004"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_005"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_006"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_007"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_009"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_010"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_012"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_013"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_015"
                    android:tint="@color/sesl_check_on" />
            </item>
        </animation-list>

    </transition>

    <transition android:toId="@id/off" android:fromId="@id/on">
        <animation-list>
            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_015"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_013"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_012"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_010"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_009"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_007"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_006"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_005"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_004"
                    android:tint="@color/sesl_check_on" />
            </item>

            <item android:duration="20">
                <bitmap
                    android:src="@drawable/sem_checkedtextview_check_to_on_mtrl_000"
                    android:tint="@color/sesl_check_off" />
            </item>
        </animation-list>
    </transition>

</animated-selector>